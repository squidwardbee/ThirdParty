{
  "name": "settler-api",
  "version": "1.0.0",
  "description": "Settler - Argument Resolution API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc && cp -r src/db/migrations dist/db/",
    "start": "node dist/index.js",
    "db:migrate": "ts-node src/db/migrate.ts",
    "db:migrate:prod": "node dist/db/migrate.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "settler",
    "api",
    "arguments"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.965.0",
    "@aws-sdk/lib-storage": "^3.965.0",
    "@aws-sdk/s3-request-presigner": "^3.965.0",
    "@elevenlabs/elevenlabs-js": "^2.29.0",
    "@tavily/core": "^0.6.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "firebase": "^12.7.0",
    "firebase-admin": "^13.6.0",
    "openai": "^6.15.0",
    "pg": "^8.16.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
